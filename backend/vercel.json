{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/docs",
            "destination": "/api/index"
        },
        {
            "source": "/openapi.json",
            "destination": "/api/index"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}